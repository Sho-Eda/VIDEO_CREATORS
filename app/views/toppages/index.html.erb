<% if logged_in? %>
<div class="top_main">  
    <div class="main_visual_login">
    </div>    

    <div class="user_post">
        <%= render "posts/posts", posts:@posts %>
    </div>
</div>

<% else %>
<div class="top_main">  
    <div class="main_visual">
    </div>    
    
    <div class="user_post">
        <%= render "posts/posts", posts:@posts %>
    </div>
</div>
<% end %>
