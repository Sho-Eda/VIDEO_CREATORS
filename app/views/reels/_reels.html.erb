<%# if reels.present? %>
<div class="reel">
        <% reels.each do |reel| %>
                <div class="col-md-3">
                        <iframe width="400" height="225" src="https://www.youtube.com/embed/<%= reel.youtube_url[32..42]%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                </div>
        <% end %>
        
</div>

<div class="posts_paginate">  
      <%= paginate reels %>
  </div>    